extends layout

block vars
    - var title = 'Desserts';
    - var updated = '2/15/17';

block content
    h1 DESSERTS
    each meal in foods
        if meal.type == 'dessert'
            div.name
                h2.name= meal.name
            div.picture
                img(src meal.picture)
            div.description
                p= meal.description
            div.price
                p= meal.price
